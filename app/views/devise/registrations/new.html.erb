<legend>Sign up</legend><br /><br />

<div class="row">
<div class="span8 offset2">
    <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => { :class => 'form-horizontal' }) do |f| %>
      <%= devise_error_messages! %>
      

      <div><%= f.label :user_name,class: 'control-label' %>
      <%= f.text_field :user_name,class: 'controls', :autofocus => true %></div><br />

      <div><%= f.label :first_name,class: 'control-label' %>
      <%= f.text_field :first_name,class: 'controls' %></div><br />

      <div><%= f.label :last_name,class: 'control-label' %>
      <%= f.text_field :last_name,class: 'controls' %></div><br />

      <div><%= f.label :email,class: 'control-label' %>
      <%= f.email_field :email,class: 'controls' %></div><br />

      <div><%= f.label :password,class: 'control-label' %>
      <%= f.password_field :password,class: 'controls' %></div><br />

      <div><%= f.label :password_confirmation,class: 'control-label' %>
      <%= f.password_field :password_confirmation,class: 'controls' %></div><br /><br />

      <center>
        <div><%= f.submit "Sign up", :class => 'btn btn-large btn-primary' %></div><br />
      </center>
    <% end %>
</div>
</div>

<%= render "devise/shared/links" %>
