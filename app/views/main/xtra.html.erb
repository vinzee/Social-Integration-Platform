<script>



	txt = "<p>Browser CodeName: " + navigator.appCodeName + "</p>";
	txt+= "<p>Browser Name: " + navigator.appName + "</p>";
	txt+= "<p>Browser Version: " + navigator.appVersion + "</p>";
	txt+= "<p>Cookies Enabled: " + navigator.cookieEnabled + "</p>";
	txt+= "<p>Platform: " + navigator.platform + "</p>";
	txt+= "<p>User-agent header: " + navigator.userAgent + "</p>";
	txt+= "<p>User-agent language: " + navigator.language + "</p>";

	$("#example").html=txt;

	$('a.printPage').click(function(){
           window.print();
           return false;
	}); 

$('#alert').click(function()
{
		bootbox.alert("Hello & Welcome to the SIP!", function() {
	});

});

$('#confirm').click(function()
{
		bootbox.confirm("Are you sure?", function() {
	});

});

$('#prompt').click(function()
{
		bootbox.prompt("What is your name??", function(result) {

		if (result === null) 
		{                                             
	    	console.log("Prompt dismissed");                              
	    } 
	  else 
	  {
	    	console.log("Your Name is :" + result);                          
	  }
	});

});

$('#custom').click(function()
{
		bootbox.dialog("I am a custom dialog", [{
    "label" : "Success!",
    "class" : "btn-success",
    "callback": function() {
        console.log("great success");
    }
}, {
    "label" : "Danger!",
    "class" : "btn-danger",
    "callback": function() {
        console.log("uh oh, look out!");
    }
}, {
    "label" : "Click ME!",
    "class" : "btn-primary",
    "callback": function() {
        console.log("Primary button");
    }
}, {
    "label" : "Just a button..."
}]);

});

	$("#t-l").click(function() {

		$('.top-left').notify({
    message: { text: 'Aw yeah, It works!' }, type: 'success'
  }).show();
	});

	$("#t-r").click(function() {

		$('.top-right').notify({
    message: { text: 'Aw yeah, It works!' }, type: 'danger'
  }).show();
	});

	$("#b-l").click(function() {

		$('.bottom-left').notify({
    message: { text: 'Aw yeah, It works!' }, type: 'info'
  }).show();
	});

	$("#b-r").click(function() {

		$('.bottom-right').notify({
    message: { text: 'Aw yeah, It works!' }, type: 'warning'
  }).show();
	});

	


