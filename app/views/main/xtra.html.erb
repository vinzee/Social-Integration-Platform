	$('#fb').hover(function(){	
		$(this).transition({
		  perspective: '1000px',
		  rotateX: '180deg'
		}); 
	}, function(){
		
		$(this).transition({
		  perspective: '1000px',
		  rotateX: '0deg'
		}); 
	});


	$('#tw').hover(function(){	
		$(this).transition({
		  perspective: '5000px',
		  rotate3d: '1,1,0,180deg'
		}); 
	}, function(){
		
		$(this).transition({
		  perspective: '1000px',
		 rotate3d: '1,1,0,0deg'
		}); 
	});


	$('#fb1').hover(function(){
		$(this).stop().transition({ scale: 2.2 });
	}, function(){
		$(this).stop().transition({ scale: 1.0 });
	});


});

</script>